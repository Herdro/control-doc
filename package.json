{
  "name": "control-doc",
  "version": "1.0.0",
  "description": "backend program to control document versioning and current status",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.js",
    "start": "node index.js",
    "lint": "eslint",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all"
  },
  "keywords": [
    "backend",
    "javascript",
    "express",
    "sequelizer"
  ],
  "author": "Pedro Herrera <pedro.herrera.bazaes@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@hapi/boom": "^9.1.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "joi": "^17.4.2",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize-cli": "^6.3.0",
    "sequelizer": "^1.1.4"
  },
  "devDependencies": {
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.4.1"
  }
}
